    "from flask import Flask, request\n",
    "\n",
    "def create_app():\n",
    "    app = Flask(__name__)\n",
    "    \n",
    "    @app.route(\"/\")\n",
    "    def home():\n",
    "        print(request.cookies)\n",
    "        return \"\"\n",
    "    \n",
    "    return app\n",
    "\n",
    "create_app().run(host=\"0.0.0.0\", port=\"5006\")"
